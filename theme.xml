	<include name="Simple Vertical" displayName="${f-simpleVertical}">./_carousels/simple_vertical.xml</include>


	</subset>


<!--		LOGOS DECO		-->

	<include>./${hideCustomLogo}/logos_deco.xml</include>

	
<!--		BACKGROUNDS VIDEO		-->

	<subset name="Background Style" displayName="${f-backgroundStyle}">


	<include name="Outrun Grid">./_backgrounds/forever/outrun.xml</include>
	<include name="Dynamic Colorset Outrun Grid" displayName="${f-dynamicColorsetOutrunGrid}">./_backgrounds/forever/forever_outrun.xml</include>	
	<include name="Custom Overlay Videos" displayName="${f-customOverlayVideo}">./_backgrounds/forever/custom_overlay_video.xml</include>
	<include name="Custom Full Screen Videos + Frame" displayName="${f-customFullVideoPlus}">./_backgrounds/forever/custom_video_plus.xml</include>
	<include name="Custom Full Screen Videos" displayName="${f-customFullVideo}">./_backgrounds/forever/custom_video.xml</include>


	</subset>

<!--		CUSTOM MARQUEES ON/OFF		-->

	<subset name="Custom Marquees" displayName="${f-customMarquee}">


	<include name="No" displayName="${f-no}">./_carousels/custom_marquee_off.xml</include>	
	<include name="yes" displayName="${f-yes}">./nope.xml</include>
	<include name="just logos" displayName="${f-justLabel}">./_carousels/custom_marquee_just_wheel.xml</include>	


	</subset>

<!--		CUSTOM GAMELIST WHEELS AND BACK WHEELS ON MARQUEE ON/OFF		-->

	<subset name="Gamelist Marquees" displayName="${f-gamelistMarquee}">


	<include name="yes" displayName="${f-yes}">./nope.xml</include>
	<include name="No" displayName="${f-no}">./_grid/custom_gamelist_marquee_off.xml</include>	
	<include name="just wheel" displayName="${f-justWheel}">./_grid/custom_gamelist_marquee_just_wheel.xml</include>	


	</subset>
	

<!--		CUSTOM LOGOS ON GAMELIST WITH FOREVER CAROUSELS		-->

	<subset name="Gamelist Permanent Custom Logos" displayName="${f-gamelistCustomLogos}">


	<include name="yes" displayName="${f-yes}">./_grid/custom_logos_gamelist.xml</include>
	<include name="No" displayName="${f-no}">./nope.xml</include>	


	</subset>


<!--		SPECIFICS SYSTEMS AND CUSTOM NAMES		-->

	<include>./_specifics/${system.theme}.xml</include>
	<include>./CUSTOMIZE/system_names/${system.theme}.xml</include>

<!--		SPECIFICS SYSTEM CUSTOMIZATION		-->

	<include>./CUSTOMIZE/illustrations/${system.theme}.xml</include>
