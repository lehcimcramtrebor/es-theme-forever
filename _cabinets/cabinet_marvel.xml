
<?xml version="1.0" encoding="UTF-8"?>
<theme>
   <formatVersion>7</formatVersion>


	<variables>

	<themedcabinetonly>themed</themedcabinetonly>									

	</variables>
	
<view name="system, basic">


		<image name="cabinet" extra="true">
			<path>${cabinetPath}cabinet_marvel.png</path>
		</image>

		<image name="cabinet_shadow" extra="true">
			<path>${hiderpi}${cabinetPath}cabinet_marvel.png</path>
		</image>
		

</view>

<view name="detailed, video, grid">


		<image name="cabinet" extra="true">
			<path>${cabinetPath}cabinet_marvel_g.png</path>
		</image>

		<image name="cabinet_shadow" extra="true">
			<path>${hiderpi}${cabinetPath}cabinet_marvel_g.png</path>
		</image>
		

</view>

<view name="system, detailed, video, grid">

      <image name="background_right" extra="true">
		 <color>a30401</color>	  	  
      </image>
 
		<image name="marquee_back" extra="true">
			<color>a30401</color>
		</image>
		
 </view>

<view name="system">
			
		<text name="system_label" extra="true">
		<pos>0.5 0.885</pos>
		<zIndex>137</zIndex>
		<fontSize>0.03</fontSize>
		</text>
 </view>
 
</theme>
